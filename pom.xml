  <repositories>
		<repository>
			<id>nexus-release</id>
			<url>${project.repository.url}/nexus/content/groups/public</url>
			<releases>
				<enabled>true</enabled>
				<updatePolicy>always</updatePolicy>
			</releases>
		</repository>
	   <repository>
			<id>nexus-snapshots</id>
			<url>${project.repository.url}/nexus/content/groups/public-snapshots</url>
			<releases>
				<enabled>false</enabled>
				<updatePolicy>always</updatePolicy>
			</releases>
			<snapshots>
				<enabled>true</enabled>
				<updatePolicy>always</updatePolicy>
			</snapshots>
		</repository>

		<repository>
			<id>nexus-release-out</id>
			<url>${project.repository.url}/nexus/content/groups/public</url>
			<releases>
